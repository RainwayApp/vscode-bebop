{
    "name": "bebop",
    "displayName": "bebop",
    "description": "An efficient schema based binary serialization format for building cross-platform, real-time applications.",
    "version": "0.2.0",
    "publisher": "rainway",
    "license": "Apache-2.0",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "bebop",
            "aliases": ["Bebop", "bebop"],
            "extensions": [".bop"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bebop",
            "scopeName": "source.bop",
            "path": "./syntaxes/bebop.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "bebop",
                "path": "./snippets/bebop.json"
            }
        ]
    }
}