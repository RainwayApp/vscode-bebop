            "description": "float32 message field",
            "body": "${1:index} -> float32 ${2:fieldName};"
        },
        "float64 message field": {
            "prefix": "mf64",
            "description": "float64 message field",
            "body": "${1:index} -> float64 ${2:fieldName};"
        },
        "string message field": {
            "prefix": "ms",
