        "guid message field": {
            "prefix": "mg",
            "description": "guid message field",
            "body": "${1:index} -> guid ${2:fieldName};"
        },
        "map message field": {
            "prefix": "mm",
            "description": "map message field",
            "body": "${1:index} -> map[${2:key}, ${3:val}] ${4:fieldName};"
