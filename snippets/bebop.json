{
    ".source.go": {
        "attribute deprecated": {
            "prefix": "adep",
            "description": "attribute deprecated",
            "body": "[deprecated(\"${1:Reason}\")]"
        },
        "attribute opcode": {
            "prefix": "aop",
            "description": "attribute opcode",
            "body": "[opcode(${1:Code})]"
        },
        "message": {
            "prefix": "msg",
            "description": "message",
            "body": "message ${1:MessageName} {\n}"
        },
        "bool message field": {
            "prefix": "mbo",
            "description": "bool field",
            "body": "${1:index} -> bool ${2:fieldName};"
        },
        "byte message field": {
            "prefix": "mby",
            "description": "byte message field",
            "body": "${1:index} -> byte ${2:fieldName};"
        },
        "uint16 message field": {
            "prefix": "mu16",
            "description": "uint16 message field",
            "body": "${1:index} -> uint16 ${2:fieldName};"
        },
        "uint32 message field": {
            "prefix": "mu32",
            "description": "uint32 message field",
            "body": "${1:index} -> uint32 ${2:fieldName};"
        },
        "uint64 message field": {
            "prefix": "mu64",
            "description": "uint64 message field",
            "body": "${1:index} -> uint64 ${2:fieldName};"
        },
        "int16 message field": {
            "prefix": "mi16",
            "description": "int16 message field",
            "body": "${1:index} -> int16 ${2:fieldName};"
